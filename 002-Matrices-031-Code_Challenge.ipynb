   "source": [
    "n = 3\n",
    "A = np.random.randn(n,n)\n",
    "AtA = A.T@A\n",
    "\n",
    "B = np.random.randn(n,n)\n",
    "BtB = B.T@B\n",
    "\n",
    "Cs = AtA + BtB # sum\n",
    "Cm = AtA @ BtB # multiplication\n",
    "Ch = AtA * BtB # Hadmard\n",
    "\n",
    "print(Cs-Cs.T)\n",
    "print('')\n",
    "print(Cm-Cm.T)\n",
    "print('')\n",
    "print(Ch-Ch.T)"
